{
  "name": "abas-demo-site",
  "version": "1.0.1",
  "description": "Content for creating ABAS branded demo sites",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-css": "./node_modules/node-sass/bin/node-sass src/main.sass dist/abas-demo-site.css",
